{{outlet}}
<h3> List Of Albums By This Artist.</h3>
<table>
  <tr>
    <td>
      <img src={{searchResults.results.[1].artworkUrl100}}>
    </td>
  </tr>
  <tr>
    <td>
      Artist Name :
    </td>
    <td>
      {{searchResults.results.[0].artistName}}
    </td>
  </tr>
  <tr>
    <td>
      Genre :
    </td>
    <td>
      {{searchResults.results.[0].primaryGenreName}}
    </td>
  </tr>
</table>

<br/><br/><br/>

<table border="1px">
  <tr>
    <th>
      AlbumCover
    </th>
    <th>
      AlbumName
    </th>
    <th>
      AlbumGenere
    </th>
    <th>
      AlbumExplicitness
    </th>
    <th>
      AlbumReleaseDate
    </th>
    <th>
      TracksCount
    </th>
    <th>
      AlbumPrice
    </th>
  </tr>
  {{#each searchResults.results as |item|}}
    <tr>
      <td>
        <img src={{item.artworkUrl60}}>
      </td>
      <td>
        {{item.collectionName}}
      </td>
      <td>
        {{item.primaryGenreName}}
      </td>
      <td>
        {{item.collectionExplicitness}}
      </td>
      <td>
        {{item.releaseDate}}
      </td>
      <td>
        {{item.trackCount}}
      </td>
      <td>
        {{item.collectionPrice}}
      </td>
    </tr>
  {{/each}}
</table>
